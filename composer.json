{
  "name": "kaystrobach/dyncss-less",
  "description": "DynCss-Parser",
  "type": "typo3-cms-extension",
  "require": {
    "typo3/cms-core": ">=7.6.15,<8.8",
    "kaystrobach/dyncss": "*"
  },
  "require-dev": {
    "typo3/cms": "^8.7",
    "kaystrobach/dyncss": "^0.8.0"
  },
  "replace": {
    "dyncss_less": "self.version",
    "typo3-ter/dyncss_less": "self.version"
  },
  "autoload": {
    "psr-4": {
      "KayStrobach\\DyncssLess\\": "Classes/"
    }
  },
  "keywords": ["dyncss", "less"],
  "homepage": "http://donate.kay-strobach.de/",
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web"
    }
  }
}
